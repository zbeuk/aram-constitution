{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"resolveJsonModule": true,
		"moduleDetection": "force",
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"paths": {
			"~/*": ["src/*"],
			"@favicon/*": ["public/favicon/*"],
			"style-system/*": ["style-system"]
		}
	},
	"include": ["src", "styled-system"]
}
